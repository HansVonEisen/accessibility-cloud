[
    {
        "type": "HTTPDownload",
        "parameters": {
            "sourceUrl": "https://dl.dropboxusercontent.com/u/5503063/ac/data/axsmapdata-v02.json"
        }
    },
    {
        "type": "ConvertToUTF8",
        "parameters": {
            "fromCharSet": "utf8"
        }
    },
    {
        "type": "Split",
        "parameters": {
            "string": "\n"
        }
    },
    {
        "type": "ParseJSONChunks",
        "parameters": {
            "path": "*",
            "length": "totalFeatures"
        }
    },
    {
        "type": "TransformData",
        "parameters": {
            "mappings": {
                "geometry": "{ type: 'Point', coordinates: [Number(row['longitude']), Number(row['latitude'])] }",
                "properties-originalId": "row.place_id",
                "properties-detailsURL": "'https://axsmap.com/venue/' + row.place_id",
                "properties-name": "row.name",
                "properties-category": "helpers.AXSMaps.getCategoryFromList(row.types)",
                "properties-address": "_.join([row.address, row.city,row.state])",
                "properties-accessibility-accessibleWith-wheelchair": "row.entry > 3",
                "properties-accessibility-areas-0-entrances-0-ratingForWheelchair": "row.entry/5",
                "properties-accessibility-areas-0-ratingSpacious": "helpers.AXSMaps.estimateRatingFor(row, row.spacious)",
                "properties-accessibility-areas-0-isWellLit": "helpers.AXSMaps.estimateFlagFor(row, row.welllit) > 0.5 ? true: undefined",
                "properties-accessibility-areas-0-isQuiet": "helpers.AXSMaps.estimateFlagFor(row, row.quiet) > 0.5 ? true: undefined",
                "properties-accessibility-areas-0-entrances-0-isMainEntrance": "helpers.AXSMaps.estimateRatingFor(row, row.secondentrance)",
                "properties-accessibility-areas-0-entrances-0-hasRemovableRamp": "helpers.AXSMaps.estimateRatingFor(row, row.ramp) > 0.5 ? true: undefined",
                "properties-accessibility-accessibleWith-guideDog": "helpers.AXSMaps.estimateFlagFor(row, row.guidedog) > 0.5 ? true: undefined",
                "properties-accessibility-parking-parkingSpacesForWheelchairUsers-areAvailable": "helpers.AXSMaps.estimateRatingFor(row, row.parking) > 0.5 ? true : undefined"
            }
        }
    },
    {
        "type": "UpsertPlace",
        "parameters": {}
    }
]