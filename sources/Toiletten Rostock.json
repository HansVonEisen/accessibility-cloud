{
"link"        : "https://geo.sv.rostock.de/download/opendata/toiletten/toiletten.csv",          
"converter"   : "csv",          
"parameters"  : {                        
    "delimiter"   : ",",
    "columns"     : true
},
"mappings"    : {
    "Id"          : "row['uuid']",
    "Name"        : "'Public toilet in Rostock, ' + row['gemeindeteil_name']",
    "Address"     : "row['strasse_name'] + ' ' + row['hausnummer'] + row['hausnummer_zusatz'] + ', ' + row['postleitzahl'] + ' Rostock'",
    "Latitude"    : "Number(row['latitude'])",
    "Longitude"   : "Number(row['longitude'])",
    "Accessible"  : "row['behindertengerecht'] == '1'"
},
"properties"  : {              
    "name"        : "Public toilets Rostock",
    "language"    : "German",
    "license"     : "CC by",
    "publisher"   : "City of Rostock",
    "coverage"    : "Rostock, Germany",
    "country"     : "de"
}
}
