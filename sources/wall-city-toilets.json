        "Longitude"   : "Number(row['lat'].replace(',','.'))",
        "Accessible"  : "row['Rollstuhl-Toilette (ja/nein)?'].includes('ja')"
    },
    "properties"  : {              
        "name"        : "Wall",
        "language"    : "German",
        "license"     : "CC",
        "publisher"   : "Wall",
        "coverage"    : "Germany",
        "country"     : "de"
    }
