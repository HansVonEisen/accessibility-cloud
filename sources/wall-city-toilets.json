    "parameters"  : {                        
        "delimiter"   : ";",
        "columns"     : true
    },
    "mappings"    : {
        "Name"        : "row['Name']",
        "Address"     : "row['Stra√üe'] + ' ' + row['Hausnummer'] + ', ' + row['PLZ'] + ' ' + row['Stadt'] + ', ' + row['Land']",
