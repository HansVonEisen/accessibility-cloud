{
"link"        : "Etablissements_Tourisme_et_handicap_juin_2016.csv",          
"converter"   : "csv",                 
"parameters"  : {                        
    "auto_parse"  : true,
    "columns"     : true,
    "delimiter"   : ";",
    "charset"     : "binary" // means latin-1
},
"mappings"    : {
    "Id"          : "row['REFERENCE']",
    "Name"        : "row['ACTIVIT'] + ' ' + row['ETABLISSEMENT']",
    "Address"     : "row['CODEPOSTAL'] + ' ' + row['ADRESSE'] + ', ' + row['REGION'] + ', ' + row['VILLE'] + ', ' + row['DEPARTEMENT']",
//    "Latitude"    : 
//    "Longitude"   : 
    "Accessible"  : "row['HANDICAPS'].includes('moteur')"
},
"properties"  : {              
    "name"        : "Tourist attractions in France",
    "language"    : "French",
    "license"     : "CC",
    "publisher"   : "???",
    "coverage"    : "France",
    "country"     : "fr"
}
}
