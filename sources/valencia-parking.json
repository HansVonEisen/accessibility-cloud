{
    "link"        : "http://mapas.valencia.es/lanzadera/opendata/aparcaminusvalidos/CSV",          
    "converter"   : "csv", 
    "parameters"  : {                        
        "delimiter"   : ";",
        "columns"     : true
    },
    "mappings"    : {
        "Id"          : "'valencia-parking-' + row['X'] + '-' + row['Y']",
        "Name"        : "row['plazas'] + ' parking spots'",
        //"Address"     : "",
        "Latitude"    : "Number( row['X'] )/10000",
        "Longitude"   : "Number( row['Y'] )/10000",
        "Accessible"  : "true"
    },
    "properties"  : {              
        "name"        : "Accessible parking spaces in the city of Valencia",
        "language"    : "",
        "license": {
            "url": "https://creativecommons.org/licenses/by/4.0/deed",
            "attribution": true,
            "share-alike": false
        },
        "publisher"   : "Mobidat",
        "coverage"    : "Valencia, Spain",
        "country"     : "es",
        "added"       : "2016-07-22",
        "contributor": {
            "name": "Valencia City Council: Parking without limitations",
            "url": "http://gobiernoabierto.valencia.es/en/dataset/?id=aparcamiento-movilidad-reducida"
        }
    }
}
