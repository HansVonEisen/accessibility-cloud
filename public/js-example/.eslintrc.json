{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 5,
      "sourceType": "script",
      "ecmaFeatures": {
          "impliedStrict": true
      }
  },
  "globals": {
    "AccessibilityCloud": true,
    "Mustache": true,
    "$": true
  },
  "env": {
    "builtin": true,
    "browser": true
  },
  "rules": {
    "object-shorthand": ["error", "never"],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-alert": [0],
    "no-console": [0],
    "prefer-arrow-callback": [0],
    "no-var": [0],
    "prefer-template": [0],
    "func-names": [0]
  }
}
